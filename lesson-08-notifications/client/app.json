{
	"expo": {
		"name": "lesson-08-notifications",
		"slug": "lesson-08-notifications",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-notifications",
				{
					"icon": "",
					"color": "#2A1938"
				}
			]
		],
		"scheme": "lesson-06-jotai"
	}
}
